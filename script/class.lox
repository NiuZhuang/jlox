class Breakfast {
    init(meat, bread){
        this.meat = meat;
        this.bread = bread;
    }
    serve(who){
        print "Enjoy your " + this.meat + " and " + this.bread + ", " + who + ".";
    }
    eat(){
        print "Crunch crunch crunch!";
    }
    sayHi(){
        print this;
    }
}

var baconAndToast = Breakfast("bacon", "toast");
baconAndToast.sayHi();
baconAndToast.serve("Sir");